<view><view class="{{['header',(isH5Plus)?'status':'']}}"><block wx:if="{{login}}"><view class="userinfo"><view class="face"><image src="{{avatarUrl}}"></image></view><view class="info"><view class="username">{{userinfo.userName}}</view><view class="integral">{{"手机号码:"+userinfo.userPhone}}</view></view></view></block><block wx:else><view class="userinfo"><view class="face"><image src="../../static/logo.png"></image></view><view class="info"><navigator url="/pages/login/login"><view class="username" style="padding-top:8px;padding-left:8px;" url="{{true}}">点击登录</view></navigator></view></view></block></view><view class="orders"><view class="box"><block wx:for="{{orderTypeLise}}" wx:for-item="row" wx:for-index="index" wx:key="name"><view class="label" hover-class="hover" data-event-opts="{{[['tap',[['toOrderType',[index]]]]]}}" bindtap="__e"><view class="icon"><block wx:if="{{row.badge>0}}"><view class="badge">{{row.badge}}</view></block><image src="{{'../../static/'+row.icon}}"></image></view>{{''+row.name+''}}</view></block></view></view><block wx:for="{{severList}}" wx:for-item="list" wx:for-index="list_i" wx:key="list_i"><view class="list"><block wx:for="{{list}}" wx:for-item="li" wx:for-index="li_i" wx:key="name"><view class="{{['li',(li_i==list.length-1)?'noborder':'']}}" hover-class="hover" data-event-opts="{{[['tap',[['toPage',[list_i,li_i]]]]]}}" bindtap="__e"><view class="icon"><image src="{{'../../static/'+li.icon}}"></image></view><view class="text">{{li.name}}</view><image src="{{'../../static/'+to.icon}}"></image></view></block></view></block></view>