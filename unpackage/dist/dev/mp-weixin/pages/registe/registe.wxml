<view class="content"><form rules="{{rules}}" data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="uni-form-item uni-column" prop="userName"><view class="title">姓名</view><input class="uni-input" name="userName" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['$0','userName','$event',[]],['user']]]]]}}" value="{{user.userName}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">密码</view><input class="uni-input" password="{{true}}" name="password" placeholder="请设置密码" data-event-opts="{{[['input',[['__set_model',['$0','userPassword','$event',[]],['user']]]]]}}" value="{{user.userPassword}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">确认密码</view><input class="uni-input" password="{{true}}" name="comfirmPwd" placeholder="请再次输入密码"/></view><view class="uni-form-item uni-column"><view class="title">性别</view><radio-group bindinput="__e" name="radio" value="{{user.userGender}}" data-event-opts="{{[['input',[['__set_model',['$0','userGender','$event',[]],['user']]]]]}}"><label><radio value="1"></radio><text>男</text></label><label><radio value="0"></radio><text>女</text></label></radio-group></view><view class="uni-form-item uni-column"><view class="title">车牌号</view><input class="uni-input" name="plate" placeholder="请输入车牌号" data-event-opts="{{[['input',[['__set_model',['$0','carId','$event',[]],['user']]]]]}}" value="{{user.carId}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">手机号</view><input class="uni-input" name="phone" placeholder="请输入联系方式" data-event-opts="{{[['blur',[['teltest']]],['input',[['__set_model',['$0','userPhone','$event',[]],['user']]]]]}}" value="{{user.userPhone}}" bindblur="__e" bindinput="__e"/></view><button form-type="submit">注 册</button></form></view>